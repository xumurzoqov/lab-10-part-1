#include <iostream>
using namespace std;
int main() {

    int temp[31], x;
    for (int i=0; i<31; i++) {
        cin >> temp[i];
    }
    cout << "Enter comparison temp:";
    cin >>x;
    int *p = temp;
    for (int i=0; i<31; i++) {
        if (*(p+i)<x) {
            cout << "First day number: " << i+1;
            break;
        }
    }
    return 0;

}
